<html>
  <head>
    <title>{{ title }}</title>
    <meta charset="UTF-8">
    <style>
{% include 'normalize.css' %}
{% include 'skeleton.css' %}
.hit {background-color: #EAFFEA}
.miss {background-color: #FFECEC}
.container .code {margin-left: 0}
pre {line-height: 1.3}
    </style>
  </head>
  <body>
    <div class="container">
      <h1>{{ title }}</h1>
      <table class="u-full-width">
        <thead>
          <tr>
          {%- for key in lines %}
            <th>{{ key }}</th>
          {%- endfor %}
          </tr>
        </thead>
        <tbody>
{%- for row_num in range(lines | get_max_number_of_values_in_dict) %}
          <tr>
          {%- for key in lines %}
            {%- if sources and key=="Filename"%}
            <td><a href="#{{ lines[key][row_num] }}">{{ lines[key][row_num] }}</a></td>
            {%- else %}
            <td>{{ lines[key][row_num] }}</td>
            {%- endif %}
          {%- endfor %}
          </tr>
{%- endfor %}
        </tbody>
        <tfoot>
          <tr>
            {%- for key in lines %}
            <td>{{ footer[key] }}</td>
            {%- endfor %}
          </tr>
        </tfoot>
      </table>
{%- if sources %}
{%- from 'macro.source.jinja2' import render_source -%}
{%- for filename, source in sources %}
<h4 id="{{ filename }}">{{ filename }}</h4>
{{ render_source(source) }}
{%- endfor %}
{% else %}
<p>{{ no_file_sources_message }}</p>
{%- endif %}
    </div>
  </body>
</html>
